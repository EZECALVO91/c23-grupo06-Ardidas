<!--ACA SE INCLUYE EL PARTIAL : HEAD -->
<%- include("./partials/head.ejs") %>

<body class="body_home">

  <!--ACA SE INCLUYE EL PARTIAL : HEADER -->
  <%- include("./partials/header.ejs") %>

    <main class="main_login">
      <form class="box_home_form" action="/login" method="post">
        <section>
          <article class="articulo_login_main">
            <h2 class="login_h_all">EMAIL</h2>
            <label for="usuario"></label>
            <input class="input_login" type="email" name="usuario" placeholder="tu-email@email.com*" required>
          </article>
          <article class="articulo_login_main">
            <h2 class="login_h_all">CONTRASEÑA</h2>
            <label for="contraseña"></label>
            <input class="input_login" name="password" type="password" placeholder=" " required>

            <a href="" class="login_contra_up">
              <h6 class="login_h_all">¿Olvidaste tu contraseña?</h6>
            </a>
            <div class="label_recordar">
              <label for="Recuerdame" class="r">Recuerdame</label>
              <input type="checkbox" name="Recuerdame">
            </div>
          </article>
          <article class="articulo_login_main">
            <button class="btn_login_main" type="submit">INICIAR SESIÓN</button>
            <a href="/register" class="btn_login_up">
              <h6>¿No tenés cuenta? Crear cuenta.</h6>
            </a>
          </article>
        </section>
      </form>
    </main>

<!--ACA SE INCLUYE EL PARTIAL : -->
<%- include("./partials/footer.ejs") %>


</body>

</html>